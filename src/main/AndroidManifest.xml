<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.pyneo.maps.applib"
	android:installLocation="auto"
	android:sharedUserId="@string/app_user_id"
	android:sharedUserLabel="@string/app_user_label"
	android:versionCode="8111"
	android:versionName="0.9.0.b1" >
	<supports-screens
		android:anyDensity="true"
		android:largeScreens="true"
		android:normalScreens="true"
		android:xlargeScreens="true"
		android:resizeable="true"
		android:smallScreens="true" />
	<uses-sdk
		android:minSdkVersion="8"
		/>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-feature android:name="android.hardware.location" android:required="false" />
	<uses-feature android:name="android.hardware.location.gps" android:required="false" />
	<uses-feature android:name="android.hardware.location.network" android:required="false" />
	<application
		android:name=".MapApplication"
		android:allowClearUserData="false"
		android:icon="@drawable/icon"
		android:label="@string/app_name" >
		<activity
			android:name=".MainActivity"
			android:label="@string/app_name"
			android:launchMode="singleTop" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.SEARCH" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
			<meta-data
				android:name="android.app.searchable"
				android:resource="@xml/searchable" />
			<intent-filter>
				<action android:name=".SHOW_POINTS" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<category android:name="android.intent.category.BROWSABLE" />
				<data android:scheme="geo" />
			</intent-filter>
		</activity>
		<activity android:name=".MainPreferences" ></activity>
		<provider
			android:name=".utils.SearchSuggestionsProvider"
			android:authorities="org.pyneo.maps.SuggestionProvider" >
		</provider>
		<activity android:name=".kml.PoiActivity" ></activity>
		<activity android:name=".kml.PoiListActivity" ></activity>
		<activity android:name=".kml.TrackListActivity" ></activity>
		<activity android:name=".kml.PoiCategoryActivity" ></activity>
		<activity android:name=".kml.PoiCategoryListActivity" ></activity>
		<activity android:name=".kml.PoiIconSetActivity" ></activity>
		<activity android:name=".kml.ImportPoiActivity" ></activity>
		<activity android:name=".kml.ImportTrackActivity" ></activity>
		<activity android:name=".kml.TrackActivity" ></activity>
		<activity android:name=".kml.TrackStatActivity" ></activity>
		<service
			android:name=".trackwriter.TrackWriterService"
			android:icon="@drawable/track_writer_service"
			android:process=":trackwriter" >
			<intent-filter>
				<action android:name=".trackwriter" />
				<action android:name=".trackwriter.IRemoteService" />
			</intent-filter>
		</service>
		<activity android:name="org.openintents.filemanager.FileManagerActivity" ></activity>
		<activity
			android:name=".downloader.AreaSelectorActivity"
			android:launchMode="singleInstance" >
		</activity>
		<service
			android:name=".applib.downloader.MapDownloaderService"
			android:icon="@drawable/track_writer_service"
			android:process=":mapdownloader" >
			<intent-filter>
				<action android:name=".downloader" />
				<action android:name=".downloader.IRemoteService" />
			</intent-filter>
		</service>
		<activity android:name=".downloader.DownloaderActivity" ></activity>
		<activity android:name=".preference.MixedMapsPreference"></activity>
		<activity android:name=".preference.UserMapsPrefActivity"></activity>
		<activity android:name=".preference.PredefMapsPrefActivity"></activity>
		<activity android:name=".preference.CustomMapsPrefActivity"></activity>
		<activity android:name=".preference.PairMapsPrefActivity"></activity>
		<activity android:name=".downloader.FileDownloadListActivity"></activity>
		<activity android:name=".geoui.GeoDataActivity" android:theme="@style/Theme.AppCompat.Light"></activity>
	</application>
</manifest>
