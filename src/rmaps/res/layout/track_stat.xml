	<org.pyneo.maps.track.ChartView
		android:id="@+id/chart"
		android:layout_width="wrap_content"
		android:layout_height="200sp"
		android:layout_marginLeft="6sp"
		android:layout_marginRight="6sp" />
</LinearLayout>
