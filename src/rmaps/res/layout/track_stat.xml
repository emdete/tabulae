<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/main"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:background="#ff555555">
	<TextView
		android:id="@+id/Name"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="3sp"
		android:text="TextView"
		android:textStyle="bold"
		android:textColor="@android:color/white"
		android:background="@color/primary"
		/>
	<TableLayout
		android:id="@+id/tableLayout1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="6sp"
		android:layout_marginRight="6sp"
		android:layout_marginTop="3sp"
		>
		<TableRow
			android:id="@+id/tableRow1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			>
			<TextView
				android:id="@+id/textView1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="10sp"
				android:text="@string/duration"
				android:textAppearance="@style/stat_title"
				/>
			<RelativeLayout
				android:id="@+id/rl0"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>
				<TextView
					android:id="@+id/duration_data"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="TextView"
					android:textAppearance="@style/stat_value"
					/>
				<TextView
					android:id="@+id/duration_unit"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignBaseline="@+id/duration_data"
					android:layout_toRightOf="@+id/duration_data"
					android:paddingLeft="5sp"
					android:text="TextView"
					android:textAppearance="@style/stat_unit"
					/>
			</RelativeLayout>
		</TableRow>
		<TableRow
			android:id="@+id/TableRow04"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			>
			<TextView
				android:id="@+id/TextView11"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="6sp"
				android:text="@string/moving_time"
				android:textAppearance="@style/stat_title"
				/>
			<RelativeLayout
				android:id="@+id/rl6"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>
				<TextView
					android:id="@+id/movetime_data"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="TextView"
					android:textAppearance="@style/stat_value"
					/>
				<TextView
					android:id="@+id/movetime_unit"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
